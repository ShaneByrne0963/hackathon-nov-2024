{
  "name": "AccessorEase",
  "version": "1.0.1",
  "description": "A browser extension to address website accessibility issues limiting fully diversified inclusivity",
  "manifest_version": 3,
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "author": "The Accessibles",
  "action": {
    "default_icon": "icons/icon48.png",
    "default_popup": "index.html",
    "default_title": "AccessorEase"
  },
  "content_scripts": [{
    "matches": ["http://*/*", "https://*/*"],
    "js": ["ruler.js", "background-images.js", "update_color.js", "script.js"]
  }]
}

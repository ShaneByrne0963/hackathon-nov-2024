{
  "name": "AccessorEase",
  "version": "1.0.1",
  "description": "A browser extension that helps correct any accessibility errors on websites",
  "manifest_version": 3,
  "author": "The Accessibles",
  "action": {
    "default_popup": "index.html",
    "default_title": "AccessorEase"
  },
  "content_scripts": [{
    "matches": ["http://*/*", "https://*/*"],
    "js": ["ruler.js", "background-images.js", "update_color.js", "script.js"]
  }]
}